{
  "name": "spinel",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:functional",
    "test:unit": "jest --testMatch '<rootDir>/src/**/*.test.ts'",
    "test:functional": "jest --testMatch '<rootDir>/test/functional/**/*.test.ts' --runInBand",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "tsc": "tsc --noEmit",
    "eslint": "eslint",
    "precommit": "npm run prettier:write && npm run tsc && npm run eslint && npm run test",
    "prepare": "husky install"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintIgnore": [
    "/.eslintrc.js",
    "/jest.config.js"
  ],
  "keywords": [],
  "author": "seihmd",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.244",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^28.1.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "date-fns": "^2.28.0",
    "eslint": "^8.18.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^25.3.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-mockito": "^2.6.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "lodash": "^4.17.21",
    "neo4j-driver": "^4.4.6",
    "reflect-metadata": "^0.1.13",
    "snake-case": "^3.0.4"
  }
}
